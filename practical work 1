import random

numbers = [] #пустой список


for _ in range(10): # количество рандомных чисел
    numbers.append(random.randint(1, 10)) # список рандомных чисел от 1 до 10

print("Список случайных чисел:", numbers)

user_number = int(input("Введи число от 1 до 10: ")) #отправляем число от 1 до 10

if user_number in numbers: #проверка, есть ли число в списке
    print(f"Число {user_number} число находится в списке.")
else:
    print(f"Число {user_number} число отсутствует в списке.")
